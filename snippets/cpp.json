	"Knuth-Morris-Pratt Algorithm": {
		"prefix": "kmp",
		"body": [
			"vector<int> kmp(string s) {",
			"    int n = s.size();",
			"    vector<int> pi(n);",
			"    for(int i = 1, j = 0; i < n; i++) {",
			"        while(j && s[j] != s[i]) {",
			"            j = pi[j - 1];",
			"        }",
			"        j += s[i] == s[j];",
			"        pi[i] = j;",
			"    }",
			"    return pi;",
			"}"
		]
