	"Binary Exponentiation": {
		"prefix": "power",
		"body": [
			"int mod = $0;",
			"",
			"ll power(ll x, ll y, ll m) {",
			"    ll res = 1;",
			"    x %= m;",
			"    while(y) {",
			"        if(y & 1) {",
			"            res = (res * x) % m;",
			"        }",
			"        x = (x * x) % m;",
			"        y = y >> 1;",
			"    }",
			"    return res;",
			"}"
		]
