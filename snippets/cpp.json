		"prefix": "scc",
		"body": [
			"vector<vector<int>> adj;",
			"vector<int> tin, low, st, comp;",
			"vector<bool> in_stack;",
			"int timer = 0, scc_cnt = 0;",
			"",
			"void dfs(int x) {",
			"    tin[x] = low[x] = ++timer;",
			"    st.pb(x);",
			"    in_stack[x] = 1;",
			"    for(auto i : adj[x]) {",
			"        if(!tin[i]) {",
			"            dfs(i);",
			"        }",
			"        if(in_stack[i]) {",
			"            low[x] = min(low[x], low[i]);",
			"        }",
			"    }",
			"    if(tin[x] == low[x]) {",
			"        while(1) {",
			"            int y = st.back();",
			"            st.pop_back();",
			"            in_stack[y] = 0;",
			"            comp[y] = scc_cnt;",
