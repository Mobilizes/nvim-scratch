		"prefix": "z_algorithm",
		"body": [
			"vector<int> z_function(string s) {",
			"    int n = s.size();",
			"    vector<int> z(n);",
			"    int l = 0, r = 0;",
			"    for(int i = 1; i < n; i++) {",
			"        z[i] = max(0, min(z[i - l], r - i + 1));",
			"        while(i + z[i] < n && s[z[i]] == s[i + z[i]]) {",
			"            l = i;",
			"            r = i + z[i];",
			"            z[i]++;",
			"        }",
			"    }",
			"    return z;",
			"}"
		]
	},
